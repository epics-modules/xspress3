    field(INPA, "$(XSPRESS3_PREFIX,undefined):C1_ROI1:ArrayRate_RBV.LB NPP NMS")   # Previous counter value
    field(INPB, "$(XSPRESS3_PREFIX,undefined):C1_ROI1:ArrayCounter_RBV NPP NMS")   # Current counter value
