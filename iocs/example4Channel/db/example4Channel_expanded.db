   field(CALC, "A*0.75")
   field(INPA, "$(XSPRESS3_PREFIX,undefined):ARR2:QueueSize")
   field(OUT,  "$(XSPRESS3_PREFIX,undefined):ARR2:QueueUse.HIGH")
   field(FLNK, "$(XSPRESS3_PREFIX,undefined):ARR2:QueueUseHIHI")
