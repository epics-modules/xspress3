# Updated rate calculation to use a smoothing factor w/ guard against negative values
record(calc, "$(XSPRESS3_PREFIX,undefined):C1_SCA5:ArrayRate_RBV")
{
    field(INPA, "$(XSPRESS3_PREFIX,undefined):C1_SCA5:ArrayRate_RBV.LB NPP NMS")   # Previous counter value
    field(INPB, "$(XSPRESS3_PREFIX,undefined):C1_SCA5:ArrayCounter_RBV NPP NMS")   # Current counter value
    field(INPC, "1.0")                                # Delta time in seconds
    field(INPD, "$(XSPRESS3_PREFIX,undefined):C1_SCA5:ArrayRate_RBV.VAL NPP NMS")  # Previous rate
    field(INPE, "0.0")                 # Smoothing factor
    field(CALC, "(D*E)/C+MAX(0,B-A)*(1-E)/C")
    field(PREC, "2" )
    field(EGU,  "Hz" )
    field(SCAN, "1 second")
