    field(INPA, "$(XSPRESS3_PREFIX,undefined):ARRSUM2:QueueSize")
    field(OUT,  "$(XSPRESS3_PREFIX,undefined):ARRSUM2:QueueFree.LOW")
    field(FLNK, "$(XSPRESS3_PREFIX,undefined):ARRSUM2:QueueUseHIGH")
