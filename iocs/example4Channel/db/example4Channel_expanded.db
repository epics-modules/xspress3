    field(INPA, "$(XSPRESS3_PREFIX,undefined):C1_ROI2:ArrayRate_RBV.LB NPP NMS")   # Previous counter value
    field(INPB, "$(XSPRESS3_PREFIX,undefined):C1_ROI2:ArrayCounter_RBV NPP NMS")   # Current counter value
