# Updated rate calculation to use a smoothing factor w/ guard against negative values
record(calc, "$(XSPRESS3_PREFIX,undefined):C4_ROI4:ArrayRate_RBV")
{
    field(INPA, "$(XSPRESS3_PREFIX,undefined):C4_ROI4:ArrayRate_RBV.LB NPP NMS")   # Previous counter value
    field(INPB, "$(XSPRESS3_PREFIX,undefined):C4_ROI4:ArrayCounter_RBV NPP NMS")   # Current counter value
